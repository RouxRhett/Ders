<!DOCTYPE html>
<html>
  <head>
    <title>Ders</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
      <p>ユーザでログイン中</p>
      <p><%= link_to :mypage, mypage_path %></p>
    <% end %>
    <% if current_admin %>
      <p>管理者でログイン中</p>
    <% end %>
    <%= flash[:notice] %>
    <%= yield %>
  </body>
</html>
